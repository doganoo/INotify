{
  "name": "doganoo/inotify",
  "description": "Simple and Leightweight library for notification services, such as email, push, sms, etc",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dogan Ucar",
      "email": "dogan@dogan-ucar.de"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "doganoo\\IN\\": "src/private/",
      "doganoo\\INotify\\": "src/public/",
      "doganoo\\INotify\\Test\\": "test/"
    }
  },
  "require": {
    "php": ">=7.1",
    "doganoo/php-algorithms": "0.*.*",
    "doganoo/php-util": "0.*.*",
    "phpmailer/phpmailer": "^6.5",
    "doganoo/simple-rbac": "1.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "6.*"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c . test"
  }
}
