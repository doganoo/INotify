{
  "name": "doganoo/inotify",
  "type": "library",
  "description": "Simple and Leightweight library for notification services, such as email, push, sms, etc",
  "license": "MIT",
  "authors": [
    {
      "name": "Dogan Ucar",
      "email": "dogan@dogan-ucar.de"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "doganoo/di-services": "dev-master",
    "doganoo/php-algorithms": "^1",
    "doganoo/php-util": "^0",
    "ezyang/htmlpurifier": "^4.13",
    "laminas/laminas-config": "^3.7",
    "laminas/laminas-servicemanager": "^3.10",
    "phpmailer/phpmailer": "^6",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.15",
    "phpunit/phpunit": "9.*"
  },
  "autoload": {
    "psr-4": {
      "doganoo\\INotify\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "scripts": {
    "test": "./vendor/bin/phpunit -c . test"
  }
}
